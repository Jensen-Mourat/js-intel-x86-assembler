{
  "name": "assembler-x86",
  "version": "1.0.0",
  "description": "An Intel x86 assembler, generates opCodes from assembly",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "nyc ./node_modules/.bin/_mocha 'tests/**/*.test.ts'",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jensen-Mourat/js-intel-x86-assembler.git"
  },
  "keywords": [
    "Assembly",
    "Js",
    "Intel",
    "x86"
  ],
  "author": "Jensen Mourat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jensen-Mourat/js-intel-x86-assembler/issues"
  },
  "homepage": "https://github.com/Jensen-Mourat/js-intel-x86-assembler#readme",
  "devDependencies": {
    "@testdeck/mocha": "^0.1.2",
    "@types/chai": "^4.2.14",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "32bit-adressing-table-modrm": "^1.0.7"
  }
}
